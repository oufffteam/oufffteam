            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>

    <Grid x:Name="MainGrid">
        <UC:View1 Margin="0,0,0,0" x:Name="view11" />
    </Grid>
</Window>
